[{"tvGuide": [{"start": 1458266400, "image": null, "end": 1458273600, "name": "Madea's Big Happy Family", "description": "Maeda the loving matriarch finds herself using her brand of tough Love to weather some rough waters."}, {"start": 1458273600, "image": null, "end": 1458280800, "name": "The Blues Brothers", "description": "Jake Blues, just out from prison, puts together his old band to save the Catholic home where he and brother Elwood were raised."}, {"start": 1458280800, "image": null, "end": 1458286200, "name": "Police Academy", "description": "With a sharp decline in the number of active police officers, the mayor orders the police academy to lift all weight, health, intelligence restrictions, allowing everyone to apply. Carey Mahoney though joins as an alternative to being sent to jail, but seems to love it…."}, {"start": 1458286200, "image": null, "end": 1458291600, "name": "Police Academy 2: Their First Assignment", "description": "Now that Mahoney and his colleagues have graduated from Police Academy they’re officially on the job as officers of the law. As the city overflows with the unrest of its latest crime wave, the boys in blue have their first assignment- to put a stop to a graffiti gang…."}, {"start": 1458291600, "image": null, "end": 1458297000, "name": "Suspect Zero", "description": "Mystery surrounds a serial killer whose victims are solely other killers."}, {"start": 1458297000, "image": null, "end": 1458304200, "name": "Return to Paradise", "description": "After a dream-time in a camp in Malaysia where a group of friends spend their vacation indulging in drinks, drugs and women, two of them return home while one of them stays behind. Yet two years later they discover that their friend is to be executed in 8 days…."}, {"start": 1458304200, "image": null, "end": 1458311400, "name": "Random Hearts", "description": "Two strangers, connected by a tragedy, both losing their partners in a same plane crash. Soon, they realize it wasn’t a coincidence.<br/><br/>."}, {"start": 1458311400, "image": null, "end": 1458313200, "name": "The Insider", "description": "A spin-off of “Entertainment Tonight,” “The Insider” is a US-syndicated entertainment news program, reporting on tabloid topics involving celebrities and Hollywood events."}, {"start": 1458313200, "image": null, "end": 1458320400, "name": "Che: Part One", "description": "In 1956, Ernesto 'Che' Guevara and a band of Castro-led Cuban exiles mobilize an army to topple the regime of dictator Fulgencio Batista."}, {"start": 1458320400, "image": null, "end": 1458327600, "name": "Che: Part Two", "description": "In 1967, Ernesto 'Che' Guevara leads a small partisan army to fight an ill-fated revolutionary guerrilla war in Bolivia, South America."}, {"start": 1458327600, "image": null, "end": 1458334800, "name": "Rendition", "description": "On a routine trip from South Africa to Washington, Egyptian-born chemical engineer Anwar El Ibrahimi suddenly disappears without a trace. As his wife Isabella begins to campaign the search for her husband, CIA analyst Freeman is having major moral issues with torture he’s witnessing…."}, {"start": 1458334800, "image": null, "end": 1458340200, "name": "Shoot 'Em Up", "description": "A late night encounter will see a man’s life take a very abrupt turn, in this Action packed Crime Comedy."}, {"start": 1458340200, "image": null, "end": 1458345600, "name": "Suspect Zero", "description": "Mystery surrounds a serial killer whose victims are solely other killers."}, {"start": 1458345600, "image": null, "end": 1458351000, "name": "Police Academy", "description": "With a sharp decline in the number of active police officers, the mayor orders the police academy to lift all weight, health, intelligence restrictions, allowing everyone to apply. Carey Mahoney though joins as an alternative to being sent to jail, but seems to love it…."}, {"start": 1458351000, "image": null, "end": 1458356400, "name": "Police Academy 2: Their First Assignment", "description": "Now that Mahoney and his colleagues have graduated from Police Academy they’re officially on the job as officers of the law. As the city overflows with the unrest of its latest crime wave, the boys in blue have their first assignment- to put a stop to a graffiti gang…."}, {"start": 1458356400, "image": null, "end": 1458363600, "name": "Return to Paradise", "description": "After a dream-time in a camp in Malaysia where a group of friends spend their vacation indulging in drinks, drugs and women, two of them return home while one of them stays behind. Yet two years later they discover that their friend is to be executed in 8 days…."}, {"start": 1458363600, "image": null, "end": 1458369000, "name": "MacGruber", "description": "The evil Dieter Von Cunth manages to get his hands on a nuclear-tipped X-5 missile, forcing the Pentagon’s hand to play its wildest final card- MacGruber. Brought in from his meditative retreat in Ecuador, MacGruber has to put together a tight team to take the fight to Von Cunth…."}, {"start": 1458369000, "image": null, "end": 1458376200, "name": "Random Hearts", "description": "Two strangers, connected by a tragedy, both losing their partners in a same plane crash. Soon, they realize it wasn’t a coincidence.<br/><br/>."}, {"start": 1458376200, "image": null, "end": 1458383400, "name": "Rendition", "description": "On a routine trip from South Africa to Washington, Egyptian-born chemical engineer Anwar El Ibrahimi suddenly disappears without a trace. As his wife Isabella begins to campaign the search for her husband, CIA analyst Freeman is having major moral issues with torture he’s witnessing…."}, {"start": 1458383400, "image": null, "end": 1458388800, "name": "Black August", "description": "Inmate activist George Lester Jackson's short life became a flashpoint for revolution, igniting the bloodiest riot in San Quentin's history."}, {"start": 1458388800, "image": null, "end": 1458397800, "name": "Freedom Song", "description": "A tale about a young man's bout in the 1960's in Mississippi before Blacks were allowed to vote and did sit ins at White establishments. The harrowing ordeals people wanting the same opportunities had to go through."}, {"start": 1458397800, "image": null, "end": 1458399600, "name": "The Insider", "description": "A spin-off of “Entertainment Tonight,” “The Insider” is a US-syndicated entertainment news program, reporting on tabloid topics involving celebrities and Hollywood events."}, {"start": 1458399600, "image": null, "end": 1458406800, "name": "Michael Collins", "description": "A biopic, of the controversial life and death of Michael Collins, The Lion of Ireland, who led the IRA against British rule."}, {"start": 1458406800, "image": null, "end": 1458414000, "name": "Philadelphia", "description": "In the role that won him his first Academy Award, Tom Hanks plays Andrew Beckett, a talented lawyer at the biggest Philadelphia law firm. Beckett first hides from his colleagues the fact that he is homosexual, then the fact that he has AIDS, then when his lesions become visible, he hides himself entirely, working from home. When an important document goes missing, the firm partners decide that this is grounds for firing him..."}, {"start": 1458414000, "image": null, "end": 1458421200, "name": "Gran Torino", "description": "Disgruntled Korean War veteran Walt Kowalski sets out to reform his neighbor, a Hmong teenager who tried to steal Kowalski's prized possession: a 1972 Gran Torino."}, {"start": 1458421200, "image": null, "end": 1458428400, "name": "Rendition", "description": "On a routine trip from South Africa to Washington, Egyptian-born chemical engineer Anwar El Ibrahimi suddenly disappears without a trace. As his wife Isabella begins to campaign the search for her husband, CIA analyst Freeman is having major moral issues with torture he’s witnessing…."}, {"start": 1458428400, "image": null, "end": 1458436500, "name": "Random Hearts", "description": "Two strangers, co